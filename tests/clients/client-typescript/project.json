{
    "name": "test-client-typescript",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "targets": {
        "integration-test": {
            "executor": "nx:run-commands",
            "outputs": [
                "{workspaceRoot}/coverage/packages/test-client-typescript"
            ],
            "cache": false,
            "options": {
                "command": "vitest run --passWithNoTests --globals",
                "cwd": "tests/clients/client-typescript"
            },
            "configurations": {
                "watch": {
                    "command": "vitest watch --passWithNoTests --globals"
                }
            }
        }
    }
}
