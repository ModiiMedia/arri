{
    "name": "test-client-typescript",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "targets": {
        "integration-test": {
            "executor": "nx:run-commands",
            "options": {
                "command": "vitest run --globals",
                "cwd": "tests/clients/client-typescript"
            }
        },
        "integration-test-old": {
            "executor": "@nx/vite:test",
            "outputs": [
                "{workspaceRoot}/coverage/packages/test-client-typescript"
            ],
            "cache": false,
            "options": {
                "passWithNoTests": true,
                "reportsDirectory": "../../../coverage/packages/test-client-typescript"
            }
        }
    }
}
