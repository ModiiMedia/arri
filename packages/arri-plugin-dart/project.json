        "test": {
            "executor": "@nx/vite:test",
            "outputs": ["coverage/packages/arri-plugin-dart"],
            "options": {
                "passWithNoTests": true,
                "reportsDirectory": "../../coverage/packages/arri-plugin-dart"
            }
        }
    },
    "tags": []
}
