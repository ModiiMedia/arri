{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "types": ["vitest/globals", "vitest/importMeta", "vite/client", "node"]
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*.test.ts",
    "./src/**/*.spec.ts",
    "./src/**/*.test.tsx",
    "./src/**/*.spec.tsx",
    "./src/**/*.test.js",
    "./src/**/*.spec.js",
    "./src/**/*.test.jsx",
    "./src/**/*.spec.jsx",
    "./src/**/*.d.ts"
  ],
  "exclude": []
}
