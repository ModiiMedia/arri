{
  "name": "go-playground",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "cache": false,
      "options": {
        "command": "arri build",
        "cwd": "playground/go"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "arri dev",
        "cwd": "playground/go"
      }
    }
  }
}
