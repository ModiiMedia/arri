{
  "name": "test-client-dart",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "targets": {
    "pub": {
      "executor": "nx:run-commands",
      "options": {
        "command": "dart pub",
        "cwd": "tests/clients/dart"
      }
    },
    "pub-get": {
      "executor": "nx:run-commands",
      "options": {
        "command": "dart pub get",
        "cwd": "tests/clients/dart"
      }
    },
    "integration-test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "dart test --reporter=expanded",
        "cwd": "tests/clients/dart"
      }
    }
  }
}
