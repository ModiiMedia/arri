    "autoroutes",
    "citty",
    "codegen",
    "Consola",
    "degit",
    "middlewares",
    "modii",
    "nitropack",
    "ofetch",
    "pathe",
    "pubspec",
    "scule",
    "struct",
    "typebox",
    "typeof",
    "uncrypto",
    "unflatten",
    "vals"
