{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "dart.analysisExcludedFolders": [
    "**/node_modules",
    "**/dist",
    "**/build",
    ".nx",
    "coverage",
    "node_modules",
    "tooling",
    "languages/kotlin",
    "languages/ts",
    "languages/cpp",
    "dist",
    "tests/server"
  ],
  "cSpell.words": [
    "Arri",
    "arrirpc",
    "autoroutes",
    "chrono",
    "citty",
    "codegen",
    "Consola",
    "degit",
    "kotlinx",
    "ktor",
    "middlewares",
    "modii",
    "nitropack",
    "ofetch",
    "pathe",
    "reqwest",
    "postversion",
    "pubspec",
    "scule",
    "struct",
    "typebox",
    "typeof",
    "uncrypto",
    "unflatten",
    "vals",
    "websockets"
  ],
  "rust-analyzer.linkedProjects": [
    "packages/arri-rs/Cargo.toml",
    "packages/arri-client-rust/Cargo.toml",
    "packages/arri-codegen/rust-reference/Cargo.toml",
    "tests/clients/rust/Cargo.toml"
  ],
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "eslint.validate": ["json"],
  "java.compile.nullAnalysis.mode": "automatic"
}
