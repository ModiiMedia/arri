{
  "name": "arri-client-dart",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/arri-client-dart/lib",
  "targets": {
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "dart pub publish -f",
        "cwd": "packages/arri-client-dart"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "dart test",
        "cwd": "packages/arri-client-dart"
      }
    },
    "pub": {
      "executor": "nx:run-commands",
      "options": {
        "command": "dart pub",
        "cwd": "packages/arri-client-dart"
      }
    },
    "dart": {
      "executor": "nx:run-commands",
      "options": {
        "command": "dart",
        "cwd": "packages/arri-client-dart"
      }
    }
  }
}
